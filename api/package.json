{
  "name": "api",
  "version": "0.0.1",
  "scripts": {
    "build:schema": "tsx src/schema.ts",
    "build": "tsc && tsc-alias",
    "db:seed": "tsx prisma/seed.ts",
    "dev": "tsx --watch-path src src/server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc -noEmit",
    "prisma": "prisma"
  },
  "dependencies": {
    "@graphql-yoga/plugin-jwt": "~3.7",
    "@graphql-yoga/subscription": "~5.0",
    "@prisma/client": "~6.6",
    "@whatwg-node/server-plugin-cookies": "~1.0",
    "class-validator": "~0.14",
    "cookie": "~1.0",
    "graphql": "~16.11",
    "graphql-fields": "~2.0",
    "graphql-scalars": "~1.24",
    "graphql-yoga": "~5.13",
    "jsonwebtoken": "~9.0",
    "reflect-metadata": "~0.2",
    "type-graphql": "2.0.0-rc.2"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3",
    "@types/jsonwebtoken": "~9.0",
    "prisma": "~6.6",
    "tsc-alias": "~1.8"
  }
}
