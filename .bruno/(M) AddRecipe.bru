post {
  url: {{baseUrl}}/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation AddRecipe($recipe: AddRecipeInput!) {
    addRecipe(recipe: $recipe) {
      title
      description
    }
  }
}

